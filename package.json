{
  "name": "bayezon-website-browser-scraper",
  "version": "1.0.0",
  "description": "Tool that automates website search interactions and captures screenshots at each stage",
  "main": "dist/server.js",
  "scripts": {
    "dev": "bun src/server.ts",
    "build": "prisma generate && tsc",
    "start": "prisma db push --accept-data-loss && bun dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist artifacts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "playwright",
    "browserbase",
    "screenshot",
    "website",
    "search",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@browserbasehq/sdk": "^2.6.0",
    "@browserbasehq/stagehand": "^3.0.8",
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.0.0",
    "playwright": "^1.40.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^10.0.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}